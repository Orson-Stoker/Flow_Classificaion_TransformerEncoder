{
  "model": {
    "encoders_layers":1,
    "encoder":{
      "key_size":2,
      "query_size":2,
      "value_size":2,
      "num_hiddens":2,
      "num_heads":1, 
      "ffn_num_hiddens":4, 
      "dropout":0.5
    },
    "classifier":{
      "dropout1":0.5,
      "dropout2":0.3,
      "classes_num_hiddens":16,
      "num_classes":4
    }

  },

  "training": {
    "max_flows":4096,
    "pcap_dir":"data",
    "learning_rate": 0.0001,
    "batch_size": 32,
    "epochs": 12,
    "gpu":1,
    "log_steps":100,
    "splits":5,
    "logging_path":"logging.csv"
  }

}